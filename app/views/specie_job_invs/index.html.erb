<h1>Listing specie_job_invs</h1>

<table>
  <tr>
    <th><%= label :specie_job_inv, :specie %></th>
    <th><%= label :specie_job_inv, :job %></th>
    <th><%= label :specie_job_inv, :frequency_rate %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @specie_job_invs.each do |specie_job_inv| %>
  <tr>
    <td><%= specie_job_inv.specie.name unless specie_job_inv.specie.blank? %></td>
    <td><%= specie_job_inv.job.name unless specie_job_inv.job.blank? %></td>
    <td><%= specie_job_inv.frequency_rate %></td>
    <td><%= link_to 'Show', specie_job_inv %></td>
    <td><%= link_to 'Edit', edit_specie_job_inv_path(specie_job_inv) %></td>
    <td><%= link_to 'Destroy', specie_job_inv, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

