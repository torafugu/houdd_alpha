<h1>Listing status_mod_invs</h1>

<table>
  <tr>
    <th><%= label :status_mod_inv, :type %></th>
    <th><%= label :status_mod_inv, :status_mod %></th>
    <th><%= label :status_mod_inv, :owner_id %></th>
    <th><%= label :status_mod_inv, :num_of_tgt %></th>
    <th><%= label :status_mod_inv, :duration %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @status_mod_invs.each do |status_mod_inv| %>
  <tr>
    <td><%= status_mod_inv.type %></td>
    <td><%= status_mod_inv.status_mod.name unless status_mod_inv.status_mod.blank? %></td>
    <td><%= status_mod_inv.owner_id %></td>
    <td><%= status_mod_inv.num_of_tgt %></td>
    <td><%= status_mod_inv.duration %></td>
    <td><%= link_to 'Show', status_mod_inv %></td>
    <td><%= link_to 'Edit', edit_status_mod_inv_path(status_mod_inv) %></td>
    <td><%= link_to 'Destroy', status_mod_inv, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

